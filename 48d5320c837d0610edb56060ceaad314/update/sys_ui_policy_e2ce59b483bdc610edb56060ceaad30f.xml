        <script_true><![CDATA[function onCondition() {
    var d = new Date();
    var text = d.toLocaleString();
    var minutes = text.substr(13, 2);
    var convertToNumber = Number(minutes) + 5;
    var convertToString = String(convertToNumber);
    var strArray = text.split('');
    strArray[13] = convertToString.charAt(0);
    strArray[14] = convertToString.charAt(1);
    var newStr = strArray.join('');
    g_form.setValue('reservation_time', newStr);
}]]></script_true>
