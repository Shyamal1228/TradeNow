        <name>Auto Populate User Data</name>
        <order/>
        <script><![CDATA[function onLoad() {
    var ga = new GlideAjax('CustomUserUtils'); 
    ga.addParam('sysparm_name', 'getUserData');
    ga.getXMLAnswer(function(response) {
        var userData = JSON.parse(response);
        if (userData) {
            g_form.setValue('first_name', userData.first_name);
            g_form.setValue('last_name', userData.last_name);
            g_form.setValue('email', userData.email);
            g_form.setValue('location', userData.location);

        }
    });
}]]></script>
