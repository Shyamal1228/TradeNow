        <name>Auto Populate User Details Client Script</name>
        <order/>
        <script><![CDATA[function onChange() {
    var userName = g_form.getValue('name'); // Change 'user_field' to the actual field name of the sys_user lookup field

    if (userName) {
        var ga = new GlideAjax('UserDetailScriptInclude');
        ga.addParam('sysparm_name', 'getUserDetails');
        ga.addParam('sysparm_user_name', userName);
        ga.getXMLAnswer(function(response) {
            var answer = response.responseXML.documentElement.getAttribute('answer');
            if (answer) {
                var userDetails = JSON.parse(answer);
                g_form.setValue('location', userDetails.location); // Change 'location_field' to the actual field name of the location field in your custom table
                g_form.setValue('email', userDetails.email); // Change 'email_field' to the actual field name of the email field in your custom table
            }
        });
    }
}
]]></script>
