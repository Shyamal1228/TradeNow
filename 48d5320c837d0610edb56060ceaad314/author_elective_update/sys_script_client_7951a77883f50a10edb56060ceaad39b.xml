    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Auto populate the logged in user data as soon as the form is loaded.</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages>Client Script Executed</messages>
        <name>Auto Populate User Data Client Script</name>
        <order/>
        <script><![CDATA[function onLoad() {
    var userSysID = g_user.userID; 
    var grUser = new GlideRecord('sys_user');
    if (grUser.get(userSysID)) {
        g_form.setValue('name', grUser.getValue('name'));
        g_form.setValue('email', grUser.getValue('email'));
        g_form.setValue('location', grUser.getValue('location'));
    }
}]]></script>
