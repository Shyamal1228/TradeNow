        <script><![CDATA[function onLoad(control, oldValue, newValue, isLoading) {

    if (isLoading || newValue == '') {

        return;

    }

    //Call a GlideAjax

    var ga = new GlideAjax('GetUserDetails');

    ga.addParam('sysparm_name', 'getUserDetails');

    ga.addParam('sysparm_user_id', newValue);

    ga.getXML(Answer);

}



function Answer(response) {

    var answer = response.responseXML.documentElement.getAttribute("answer");

    var userObj = JSON.parse(answer);

    g_form.setValue('name', userObj.name);
	g_form.setValue('email', userObj.email);
	g_form.setValue('location', userObj.location);

}]]></script>
